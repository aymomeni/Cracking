package chapter2.Tests;

import static org.junit.Assert.*;

import org.junit.Test;

import chapter2.ListNode;
import chapter2.SumOfLists;

public class SumOfListsTests {

	@Test
	public void test1() {
		
		int[] val1Ar = {7, 1, 6};
		int[] val2Ar = {5, 9, 2};
		
		ListNode l1 = new ListNode(7);
		ListNode l2 = new ListNode(5);
		
		l1.appendToTail(1);
		l1.appendToTail(6);
		l2.appendToTail(9);
		l2.appendToTail(5);
		
		ListNode check1 = l1;
		ListNode check2 = l2;
		while(check1 != null){
			System.out.print(check1.data + " ");
			check1 = check1.next;
		}
		System.out.println();
		
		while(check2 != null){
			System.out.print(check2.data + " ");
			check2 = check2.next;
		}
		
		SumOfLists s = new SumOfLists();
		ListNode l = s.sumOfLists(l1, l2);
		
		while(l != null){
			System.out.print(l.data + " ");
			l = l.next;
		}
		
		
	}

}
