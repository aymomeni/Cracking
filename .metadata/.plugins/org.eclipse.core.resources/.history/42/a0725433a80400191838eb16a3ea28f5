package chapter3;

import java.util.Date;

import chapter3.Animal;

public class Dog extends Animal{
	
	private Date myDate;
	private String name = null;
	
	public Dog(String name){
		myDate = new Date();
		this.name = name;
	}
	
	public Date getDate(){
		return this.myDate;
	}
	
	public String getName(){
		return this.name;
	}
}