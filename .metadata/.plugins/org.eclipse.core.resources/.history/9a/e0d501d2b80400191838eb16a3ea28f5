package chapter3;

import java.util.Date;
import chapter3.Animal;

public class Cat extends Animal{
	private Date myDate;
	private String name = null;
	
	public Cat(String name){
		this.name = name;
	}
	
	public Date getDate(){
		return this.myDate;
	}
	
	public void setDate(){
		this.myDate = new Date();
	}
	
	public String getName(){
		return this.name;
	}
}
